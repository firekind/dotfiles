@import "base.rasi"

* {
    font: "Source Code Pro Semibold 11";
}

#window {
    fullscreen: false;
    anchor: south east;
    location: south east;
    y-offset: -55;
    x-offset: -10;
}

inputbar {
    children: [ prompt ];
}

#listview {
    scrollbar: false;
}



element normal.active, element selected.active, element alternate.active {
    background-color: #363636;
    text-color: #f9f9f9;
}